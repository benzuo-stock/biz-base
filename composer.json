{
    "name": "benzuo-stock/biz-base",
    "description": "a base(minimal) biz framework layer for symfony",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Benzuo\\Biz\\Base\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "TestProject\\": "tests/TestProject/src/"
        }
    },
    "require": {
        "php": "^7.1",
        "pimple/pimple": "^3.0",
        "doctrine/dbal": "^2.7.0",
        "davedevelopment/phpmig": "^1.4.0",
        "psr/log": "^1.0.0",
        "psr/cache": "^1.0.0",
        "symfony/event-dispatcher": "^3.4.0 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/cache": "^3.4.0 || ^4.0"
    }
}
